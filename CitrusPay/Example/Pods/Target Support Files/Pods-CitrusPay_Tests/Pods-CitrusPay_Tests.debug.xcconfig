FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../../../releases-for-manual-usage-v2.0.0"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CitrusPay"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CitrusPay"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CitrusPay"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-CitrusPay_Tests
PODS_ROOT = ${SRCROOT}/Pods